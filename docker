pipeline {
    agent any
    stages {
        stage('Build') { 
            steps {
                 echo 'Hello, Maven' 
				 }
        }
        stage('Deploy') {
		     steps {
                echo 'this is deploy stage'
            }
		}
		 stage('test') { 
            steps {
                 echo 'Hello, Maven' 
				 }
        }
    }
}
